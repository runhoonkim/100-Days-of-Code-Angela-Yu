def jump_rev3():
    if front_is_clear() and wall_on_right():
        move()
    elif right_is_clear():
        turn_right()
        move()
    elif wall_in_front():
        turn_left()

# while not at_goal():
    # jump_rev3()

def jump_rev4():
    while right_is_clear():
        turn_right()
        move()
    if front_is_clear() and wall_on_right():
        move()
    elif wall_in_front() and wall_on_right():
        turn_left()
        
while not at_goal():
    jump_rev4()
